// Superclass
class Animal {
    // Constructor of the superclass
    public Animal(String name) {
        System.out.println("Animal's name is: " + name);
    }
}

// Subclass
class Dog extends Animal {
    // Constructor of the subclass, calling superclass constructor
    public Dog(String name) {
        // Calling the superclass constructor using super()
        super(name);
        System.out.println("Dog's name is: " + name);
    }
}

// Main class
public class SuperClassConstructorExample {
    public static void main(String[] args) {
        // Creating an object of the Dog class, which will call both superclass and subclass constructors
        Dog dog = new Dog("Buddy");
    }
}
